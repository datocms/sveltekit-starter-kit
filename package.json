{
  "name": "sveltekit-starter-kit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate-schema": "dotenv -c -- bash -c 'gql.tada generate schema https://graphql.datocms.com --header \"X-Exclude-Invalid: true\" --header \"Authorization: $PRIVATE_DATOCMS_PUBLISHED_CONTENT_CDA_TOKEN\"'",
    "generate-cma-types": "dotenv -c -- bash -c 'npx @datocms/cli schema:generate src/lib/datocms/cma-types.ts --api-token=$PRIVATE_DATOCMS_CMA_TOKEN'",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "npx simple-git-hooks && npm run generate-schema && npm run generate-cma-types"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/jsdom": "^27.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "simple-git-hooks": "^2.13.1",
    "svelte": "^5.45.8",
    "svelte-check": "^4.3.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  },
  "type": "module",
  "dependencies": {
    "@datocms/cda-client": "^0.2.10",
    "@datocms/cma-client": "^5.2.6",
    "@datocms/svelte": "^4.2.9",
    "@mux/mux-player": "^3.10.1",
    "datocms-structured-text-to-plain-text": "^5.1.7",
    "datocms-structured-text-utils": "^5.1.7",
    "gql.tada": "^1.9.0",
    "highlight.js": "^11.11.1",
    "jsonwebtoken": "^9.0.3",
    "node-html-parser": "^7.0.1",
    "serialize-error": "^12.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run format"
  },
  "overrides": {
    "cookie": "^0.7.2"
  }
}
